{
    "presets": [
        [
            "env", {
                "targets": {
                    "browsers": ["last 5 versions", "ie >= 10"]
                }
            }
        ],
        "react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
    ],
    "env": {
        "production": {
            "presets": ["minify"]
        }
    }
}